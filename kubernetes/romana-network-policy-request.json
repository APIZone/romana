{
   "metadata": {
     "name" : "policy1"
   },
   "apiVersion" : "romana.io/demo/v1",
   "kind" : "NetworkPolicy",
   "spec" : { 
               "podSelector" : { 
                    "romana.io/tenant" : "t1" 
                },
                "allowIncoming" : {
                    "toPorts" : [
			{
		          "port": 8080,
			  "protocol": "TCP"
			},
			{
		          "port": 8000,
			  "protocol": "TCP"
			}
                    ],
                    "from" : [
			{
			   "pods" : {
				"access" : "admin"
	                   }
			}
                    ]
                }
           }
}
